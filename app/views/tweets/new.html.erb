<div class="praost-container">
    <p class="title">投稿フォーム</p>
    <%= form_for(@tweet, :url => { controller:'tweets', action:'create'})do |f| %>
        <%= f.label :title %><br>
        <%= f.text_field :title,size: 140%><br>
        <%= f.label :投稿内容 %><br>
        <%= f.text_field :body,size: 140%><br>
        <div class="field">
            <%= f.label :image %>
            <%= f.file_field :image %>
        </div>

        <%= f.submit "送信"%><br>
    <% end %>
</div>